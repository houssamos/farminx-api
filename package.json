{
  "name": "farminx-stats",
  "version": "1.0.0",
  "description": "API Node.js pour l'affichage de statistiques agricoles.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "debug": "nodemon --inspect src/server.js",
    "prisma": "prisma",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "seed": "node prisma/seed.js",
    "prisma:init": "dotenv -e .env -- npx prisma migrate dev --name init",
    "db:reset": "dotenv -e .env -- npx prisma migrate reset --force",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.10.1",
    "prisma": "^5.0.0",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "eslint": "^9.29.0",
    "jest": "^30.0.3",
    "nodemon": "^3.0.0"
  },
  "author": "",
  "license": "MIT"
}
